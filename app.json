{
  "expo": {
    "name": "xpense",
    "slug": "xpense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoX.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#55847A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#55847A"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      {
        "enableFTS": true,
        "useSQLCipher": true,
        "android": {
          "enableFTS": false,
          "useSQLCipher": false
        },
        "ios": {
          "customBuildFlags": [
            "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
          ]
        }
      }
    ]
  }
}